{
  "buildCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/images/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "^/(?!.*\\.).*$",
      "dest": "index.html"
    }
  ]
}
